<?xml version="1.0" encoding="UTF-8" ?>



<suite name = "thread" parallel = "tests" thread-count = "2">

    <!--
    tests级别：不同的test tag下的用例可以在不同的线程下执行
              相同的test tag下的用例只能在相同的线程下执行

    -->
    <test name = "demo1">
        <classes>

            <class name = "com.course.testng.mutiThread.MutiThreadOnXml"/>
        </classes>
    </test>

    <test name = "demo2">
        <classes>
            <class name = "com.course.testng.mutiThread.MutiThreadOnXml"/>
        </classes>
    </test>
</suite>